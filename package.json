{
  "name": "mikroorm-test",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "start": "webpack --mode production --config webpack.build.config.js && electron --noDevServer ./dist/main.js",
    "dev": "npm run build && webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "webpack --mode production --config webpack.build.config.js",
    "postinstall": "electron-rebuild -f -w sqlite3",
    "package": "npm run build",
    "package-mac": "npm run build && electron-builder build --mac",
    "package-linux": "npm run build && electron-builder build --linux",
    "package-win": "npm run build && electron-builder build --win --x64",
    "postpackage": "electron-packager ./ --out=./builds",
    "knex": "knex"
  },
  "mikro-orm": {
    "useTsNode": true,
    "tsConfigPath": "./tsconfig.orm.json"
  },
  "dependencies": {
    "@types/react-redux": "^7.1.9",
    "@types/sqlite3": "^3.1.6",
    "electron-devtools-installer": "^3.1.1",
    "knex": "^0.21.2",
    "liquibase": "^1.0.1",
    "mikro-orm": "^3.6.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^5.0.0",
    "ts-node": "^8.10.2"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-react": "^7.10.4",
    "@types/node": "^14.0.24",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "babel-loader": "^8.1.0",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^3.6.0",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "electron-packager": "^15.0.0",
    "electron-rebuild": "^1.11.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
